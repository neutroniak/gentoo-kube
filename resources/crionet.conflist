{
	"cniVersion": "0.4.0",
	"name": "crionet",
	"plugins": [
		{
			"name": "crionet",
			"type": "bridge",
			"bridge": "cni0",
			"isGateway": true,
			"ipMasq": true,
			"promiscMode": true,
			"ipam": {
				"type": "host-local",
				"ranges": [
					[{ "subnet": "10.88.0.0/16" }]
				],
				"routes": [{ "dst": "0.0.0.0/0" }]
			}
		},
		{
			"type": "portmap",
			"capabilities": {"portMappings": true},
			"externalSetMarkChain": "KUBE-MARK-MASQ"
		}
  ]
}

